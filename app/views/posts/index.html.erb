<div class="row">

  <div class="small-8 large-centered columns text-center mmargin mmargin-top">
<h1>Listing posts</h1>

<table>
  <thead>
    <tr>
      <!--<th>Title</th>-->
      <!--<th>Description</th>-->
      <th>Owner</th>
      <th>Parking</th>
      <th>Urgency</th>
      <th>Budget</th>
      <th>Quote</th>
      <th>How did you here about us</th>
      <th>User</th>
      <th>Photo of the problem</th>
      <th>Show Post</th>
      <th>Edit Post</th>
      <th>Delete Post</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <!--<td><%= post.title %></td>-->
        <!--<td><%= post.description %></td>-->
        <td><%= post.owner %></td>
        <td><%= post.parking %></td>
        <td><%= post.urgency %></td>
        <td><%= post.budget %></td>
        <td><%= post.quote %></td>
        <td><%= post.how_did_you_here_about_us %></td>
        <td><%= post.user_id %></td>
        <td><%= image_tag post.photo,:class => "style_image" %></td>

        <td><%= link_to 'Show', post, :class => 'button radius' %></td>
        <td><%= link_to 'Edit', edit_post_path(post), :class => 'button radius' %></td>
        <td><%= link_to 'Delete', post, method: :delete, data: { confirm: 'Are you sure?' }, :class => 'button radius' %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Post', new_post_path, :class => 'button radius' %>
</div>
    </div>